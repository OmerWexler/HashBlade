    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: utils",
            "type": "python",
            "request": "launch",
            "module": "utils"
        },
        {
            "name": "Python: client",
            "type": "python",
            "request": "launch",
            "module": "client"
        },
        {
            "name": "Python: server",
            "type": "python",
            "request": "launch",
            "module": "server"
        },
        {
            "name": "Python: communication",
            "type": "python",
            "request": "launch",
            "module": "communication"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Launch Server",
            "type": "python",
            "request": "launch",
            "program": "${cwd}/__main__.py",
            "args": [
                "--type", "server",
                "--log", "INFO",
                "--target", "EC9C0F7EDCC18A98B1F31853B1813301" // ??
                // "--target", "B42F7369B0479661D32D6B376E5AB4B8" // 6034958688
                // "--target", "EE0A47112DFD0F3CE932206E7D23794C" // 9984726412
                // "--target", "E0EC043B3F9E198EC09041687E4D4E8D" // 9999999999
                // "--target", "322EB11C76D2808156976FF47163F1A7" // 9999785635
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Launch Client",
            "type": "python",
            "request": "launch",
            "program": "${cwd}/__main__.py",
            "args": [
                "--type", "client",
                "--log", "DEBUG",
            ],
            "console": "integratedTerminal"
        }
    ]
}